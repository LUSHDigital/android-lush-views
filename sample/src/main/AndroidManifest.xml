<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
		  package="com.lush.views.sample">

	<application
		android:allowBackup="true"
		android:icon="@mipmap/ic_launcher"
		android:label="@string/app_name"
		android:roundIcon="@mipmap/ic_launcher_round"
		android:supportsRtl="true"
		android:theme="@style/AppTheme">
		<activity android:name="com.lush.views.MainActivity">
			<intent-filter>
				<action android:name="android.intent.action.MAIN"/>

				<category android:name="android.intent.category.LAUNCHER"/>
			</intent-filter>
		</activity>
		<activity android:name=".QuantitySampleActivity" />
		<activity android:name=".SpinnerSampleActivity" />
		<activity android:name=".RatingSampleActivity" />
		<activity android:name=".FormSampleActivity" />
		<activity android:name=".YoutubeSampleActivity" />
		<activity android:name=".WebViewSampleActivity" />
		<activity android:name=".ImageSampleActivity" />
	</application>

</manifest>